{
  "title": "Trading Service Performance",
  "panels": [
    {
      "type": "graph",
      "title": "Celery Queue Depth",
      "targets": [
        {"expr": "celery_queue_depth"}
      ],
      "id": 1
    },
    {
      "type": "graph",
      "title": "Worker Latency (95th %)",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(poll_trades_task_duration_seconds_bucket[5m])) by (le))"}
      ],
      "id": 2
    },
    {
      "type": "graph",
      "title": "Broker API Request Duration (95th %)",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(broker_api_request_duration_seconds_bucket[5m])) by (le, broker))"}
      ],
      "id": 3
    }
  ]
}
